<%- include('header') -%>
<div class="padding-top">
    <div class="container">
        <h2 class="model-view-header">Details of Order id - <%= data.id %></h2>
        <div>
            <ul>
                <li><span>User: </span><%= data.user %></li>
                <li><span>Address: </span><%= data.address %></li>
                <li><span>Status: </span><%= data.status %></li>
            </ul>
            <ul>
                <% data.food.forEach(x=>{%>
                    <li><%= x %></li>
                <% })%>
            </ul>
        </div>
    </div>
    
</div>
<%- include('footer') -%>